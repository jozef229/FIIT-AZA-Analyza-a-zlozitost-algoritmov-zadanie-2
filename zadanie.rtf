\f1\fs26 \cf2 \cb3 int Plength=1000000;\
char P[1000001];\
void generateP()\{\
    P[0]='a';\
    long read=0,write=1;\
    while(Plength>write) \{\
        if (read % 39 ==38) read++;\
        char r=P[read++];\
        if (r=='a') \{P[write++]='b';P[write++]='a';P[write++]='a';\}\
        else if (r=='b') \{P[write++]='a';P[write++]='c';\}\
        else if (r=='c') \{P[write++]='b';\};\
    \}\
    P[Plength]=0;\
\}\
